<templates xml:space="preserve">
    <t t-name="awesome_owl.todo_item" owl="1">
        <div class="card">
        <t t-if="!props.todo.isCompleted">
            <div class="card">
            
                <p><t t-out="props.todo.id"/>. <t t-out="props.todo.description"/></p>
            </div>
        </t>
        </div>
    </t>
</templates>